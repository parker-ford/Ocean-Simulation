#pragma kernel CSMain

float resolution;
RWStructuredBuffer<float2> freqData;
RWStructuredBuffer<float4> pingpong;

[numthreads(1,1,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    float2 freq = freqData[id.x + id.y * resolution];
    float4 data = float4(freq, 0, 0);
    pingpong[id.x + id.y * resolution] = data;
}
